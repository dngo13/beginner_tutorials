<launch>
    <arg name="frequency" default="10" />

    <node
        pkg="beginner_tutorials"
        name="talker_node"
        type="talker"
        required="true"
        output="screen"
        args = "$(arg frequency)"
    />

    <node
        pkg="beginner_tutorials"
        name="listener_node"
        type="listener"
        respawn="true"
        output="screen"
    />
</launch>